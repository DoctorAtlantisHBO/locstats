{
    "python": {
        "official_name": "Python",
        "extensions": [ ".py" ],
        "comments": {
            "single_line": [ "#" ],
            "multi_line": [
                [ "\"\"\"", "\"\"\"" ]
            ]
        }
    },
    "c": {
        "official_name": "C",
        "extensions": [ ".c", ".h" ],
        "comments": {
            "single_line": [ "//" ],
            "multi_line": [
                [ "/*", "*/" ]
            ]
        }
    },
    "c++": {
        "official_name": "C++",
        "extensions": [ ".cpp", ".hpp", ".cc", ".hh" ],
        "comments": {
            "single_line": [ "//" ],
            "multi_line": [
                [ "/*", "*/" ]
            ]
        }
    },
    "java": {
        "official_name": "Java",
        "extensions": [ ".java" ],
        "comments": {
            "single_line": [ "//" ],
            "multi_line": [
                [ "/*", "*/" ]
            ]
        }
    },
    "javascript": {
        "official_name": "JavaScript",
        "extensions": [ ".js" ],
        "comments": {
            "single_line": [ "//" ],
            "multi_line": [
                [ "/*", "*/" ]
            ]
        }
    },
    "latex": {
        "official_name": "LaTeX",
        "extensions": [ ".tex" ],
        "comments": {
            "single_line": [ "%" ],
            "multi_line": [
                [ "\\iffalse", "\\fi" ],
                [ "\\comment", "\\endcomment" ],
                [ "\\begin{comment}", "\\end{comment}" ]
            ]
        }
    },
    "vhdl": {
        "official_name": "VHDL",
        "extensions": [ ".vhd", ".vhdl" ],
        "comments": {
            "single_line": [ "--" ],
            "multi_line": []
        }
    }
}
